{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "indent": ["error", 2],
        "no-multi-spaces": ["error"],
        "linebreak-style": ["error", "windows"],
        "camelcase": "off"
    }
}
